{
  "expo": {
    "name": "onlyjubarex",
    "slug": "onlyjubarex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSMicrophoneUsageDescription": "$(PRODUCT_NAME) needs access to your Microphone."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jubarex.onlyjubarex"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) deixa eu te ver?",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) quero te ouvir também!"
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) posso roubar seus dados?"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "vou ver sua galeria só de zoas beleza."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitiria $(PRODUCT_NAME) acessar suas fotos?",
          "savePhotosPermission": "Você deixa $(PRODUCT_NAME) salvar fotos na galeria?",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "$(PRODUCT_NAME) deixa eu te ver?",
          "microphonePermission": "o $(PRODUCT_NAME) pode te ouvir?",
          "recordAudioAndroid": true
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
