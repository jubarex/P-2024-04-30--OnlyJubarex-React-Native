{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noImplicitAny":false,
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/CameraExpo.js",
    "app/(tabs)/TelaPerfil.js",
    "app/(tabs)/TelaPagamento.js",
    "app/(tabs)/CameraAlbum.tsx",
    "app/screens/CameraAlbum.jsx"
  ]
}
